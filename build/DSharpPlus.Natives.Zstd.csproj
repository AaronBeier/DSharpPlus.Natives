<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Provides the dynamic library files for zstd.</Description>
    <PackageTags>discord, discord-bot, discord-api, dsharpplus, zstd</PackageTags>
    <Authors>zstd contributors</Authors>
    <PackageLicenseFile>zstd-license.txt</PackageLicenseFile>
  </PropertyGroup>
  <ItemGroup>
    <None Include="../lib/zstd/**/*" Pack="true" PackagePath="runtimes" />
    <None Include="../licenses/zstd-license.txt" Pack="true" PackagePath="" />
  </ItemGroup>
</Project>